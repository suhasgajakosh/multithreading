package synchonization;

class ThreadClass extends Thread {
	
	private boolean running = true;

	@Override
	public void run() {
		while (running) {
			System.out.println("Hello");
			Thread.sleep(100);
		}
	
	}
	
	
}


public class Synchronization1 {

	public static void main(String[] args) {

	}

}
