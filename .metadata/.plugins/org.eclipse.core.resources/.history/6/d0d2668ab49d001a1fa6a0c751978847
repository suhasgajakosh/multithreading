package synchonization;

public class SynchExample {

	private static int count = 0;
	
	public static void main(String[] args) {
		
		doWork();

		
	}

	public static void doWork() {
Thread t1 = new Thread(new Runnable() {
			
			@Override
			public void run() {

				for (int i=0;i<10000;1++) {
					count++;
				}
			}
		});		
	}

}
